                            <MenuItem mnemonicParsing="false" text="Add Multiple Item" />
                            <MenuItem mnemonicParsing="false" text="Import From File" />
                          </items>
                        </SplitMenuButton>
                     </children>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
